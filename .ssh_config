Host *
    ServerAliveInterval 30

Host github
    HostName github.com
    User git
    IdentityFile ~/.ssh/github_rsa

Host gitlab
    HostName gitlab.com
    User git
    IdentityFile ~/.ssh/gitlab_rsa

